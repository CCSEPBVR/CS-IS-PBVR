MPICXXFLAGS = -O3 -axMIC-AVX512 -qopenmp -ipo -qopt-report=5

MPICC = mpiicc
MPICCFLAGS = -O3 -axMIC-AVX512 -qopenmp -ipo -qopt-report=5

MPILD = mpiicpc
MPILDFLAGS = -O3 -axMIC-AVX512 -qopenmp -ipo -qopt-report=5

NTL_CC  = icc
